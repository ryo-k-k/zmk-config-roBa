// roBa_dongle.overlay

/ {
    chosen {
        zmk,kscan = &kscan0;
    };

    kscan0: kscan_0 {
        compatible = "zmk,kscan-gpio-matrix";
        diode-direction = "col2row";

        // 【修正点】中身は空でも、プロパティ自体は必須のため記述する
        row-gpios = <>;
        col-gpios = <>;
    };

    // matrix_transform は前回と同じ
    matrix_transform: matrix_transform_0 {
        compatible = "zmk,matrix-transform";
        rows = <1>;
        columns = <1>;
        map = <>;
    };
};
